	config PACKAGE_lftp
		tristate "lftp.. a sophisticated file transfer program with command line interface."
		default y if DEFAULT_lftp
		default m if ALL
		select PACKAGE_libexpat
		select PACKAGE_zlib
		select PACKAGE_libpthread if USE_GLIBC
